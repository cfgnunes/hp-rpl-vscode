{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "HP UserRPL",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#operators"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.userrpl",
					"match": "@.*$"
				},
				{
					"name": "comment.block.documentation.userrpl",
					"begin": "%%HP",
					"end": ";",
					"patterns": [
						{
							"match": "\\\\;",
							"name": "constant.character.escape.userrpl"
						}
					]
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.userrpl",
					"match": "\\b(CASE|DIR|DO|ELSE|END|FOR|IF|IFERR|IFT|IFTE|NEXT|REPEAT|START|STEP|THEN|UNTIL|WHILE)\\b"
				},
				{
					"name": "keyword.other.userrpl",
					"match": "(\u0085DAT|\u0085PAR|\u008CENTER|\u00DFENTER)\\b"
				},
				{
					"name": "keyword.other.userrpl",
					"match": "\\b(ALRMDAT|CASDIR|CASINFO|ENVSTACK|CST|der1|der2|der3|EPS|EQ|EXITED|EXPR|IERR|IOPAR|MASD\\.INI|MODULO|MHpar|Mpar|n1|n2|n2|n3|n4|Nmines|PERIOD|PPAR|PRIMIT|PRTPAR|REALASSUME|s1|s2|s3|s4|STARTED|STARTEQW|STARTERR|STARTOFF|STARTRECV|STARTSEND|STARTUP|TOFF|TPAR|VPAR|VX|ZPAR)\\b"
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric.userrpl",
					"match": "\\b(\\.)?[0-9]+[E]?\\b"
				},
				{
					"name": "constant.numeric.binary.userrpl",
					"match": "# [0-9a-fA-F]+[bdho]\\b"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.double.userrpl",
					"begin": "\"",
					"end": "\"",
					"patterns": [
						{
							"match": "\\\\\"",
							"name": "constant.character.escape.userrpl"
						}
					]
				},
				{
					"name": "string.quoted.single.algebraic",
					"begin": "'",
					"end": "'",
					"patterns": [
						{
							"match": "\\\\'",
							"name": "constant.character.escape.userrpl"
						}
					]
				},
				{
					"name": "string.quoted.single.algebraic50",
					"begin": "`",
					"end": "`",
					"patterns": [
						{
							"match": "\\\\`",
							"name": "constant.character.escape.userrpl"
						}
					]
				}
			]
		},
		"functions": {
			"patterns": [
				{
					"name": "support.function.userrpl",
					"match": "(_|,|;|FC\\?C|FS\\?C)"
				},
				{
					"name": "support.function.userrpl",
					"match": "(\u0085\\-|\u0085\\+|\u0085X\\*Y|\u0085X\\^2|\u0085X2|\u0085XY|\u0085Y\\^2|\u0085Y2|A\u008DH|B\u008DR|C\u008DPX|C\u008DR|D\u008DR|EQ\u008D|H\u008DA|H\u008DS|H\u008D|I\u008DR|N\u0085|PX\u008DC|\u008DQ|\u008DQ\u009C|R\u008DB|R\u008DC|R\u008DD|R\u008DI|S\u008DH|UFL1\u008DMINIF|V\u008D|\u008DV2|\u008DV3|\u0085X|\u0085Y|\u0085LINE|\u0085LIST|\u008DALG|ARM\u008D|ARRY\u008D|\u008DARRY|CD\u008D|\u008DCD|CL\u0085|COL\u008D|\u008DCOL|COL\u0085|COMP\u008D|DIAG\u008D|\u008DDIAG|F0\u0096|FONT\u008D|\u008DFONT|HEADER\u008D|\u008DHEADER|HMS\u008D|\u008DHMS|KEYTIME\u008D|\u008DKEYTIME|LANGUAGE\u008D|\u008DLANGUAGE|LCD\u008D|\u008DLCD|LIST\u008D|\u008DLIST|\u009BLIST|\u009CLIST|\u008DLST|\u008FMATCH|\u0090MATCH|MINIFONT\u008D|\u008DMINIFONT|\u008DNDISP|OBJ\u008D|\u008DPRG|\u008DRAM|RCL\u0085|ROW\u008D|\u008DROW|\u008DS2|SCL\u0085|STO\u0085|STR\u008D|\u008DSTR|\u008DTAG|\u008DUNIT|A\u008D|\u008DA|A\u008D|\u008DA|\u008DH|MAX\u0085|MIN\u0085)"
				},
				{
					"name": "support.function.userrpl",
					"match": "\\b(COL\\-|COL\\+|DATE\\+|FC\\?|FS\\?|HMS\\-|HMS\\+|ISPRIME\\?|LC~C|LR~R|PIX\\?|R~SB|ROW\\-|ROW\\+|RPL>|S~N|SB~B|STO\\-|STO\\*|STO\\/|STO\\+|XLIB~)"
				},
				{
					"name": "support.function.userrpl",
					"match": "(\\*H|\\*W|%CH|%T)\\b"
				},
				{
					"name": "support.function.userrpl",
					"match": "\\b(ABCUV|ABS|abscisse|ACK|ACKALL|ACOS|ACOS2S|ACOSH|ADD|ADDTMOD|ADDTOREAL|afficher|aire|ALGB|ALOG|AMORT|AND|angle|ANIMATE|ANS|APEEK|APPLY|ARC|ARCHIVE|ARG|ARIT|ASIN|ASIN2C|ASIN2T|ASINH|ASM|ASN|ASR|ASSUME|ATAN|ATAN2S|ATANH|ATICK|ATTACH|AUGMENT|AUTO|AXES|AXL|AXM|AXQ|BAR|BARPLOT|BASIS|BAUD|BEEP|BESTFIT|BetaTesting|BIN|BINS|bissectrice|BLANK|BOX|BUFLEN|BYTES|C2P|CASCFG|CASCMD|CEIL|CENTR|cercle|CF|CHINREM|CHOLESKY|CHOOSE|CHR|CIRC|CKSM|CLEAR|CLKADJ|CLLCD|CLOSEIO|CLUSR|CLVAR|CMPLX|CNRM|COLCT|COLLECT|COMB|CON|COND|CONIC|CONJ|CONLIB|CONST|CONSTANTS|CONT|CONVERT|CORR|COS|COSH|COV|CR|CRC|CRDIR|CRLIB|CROSS|CSWP|CURL|CYCLOTOMIC|CYLIN|DARCY|DATE|dB|DBUG|DDAYS|DEC|DECR|DEDICACE|DEF|DEFINE|DEG|DEGREE|DELALARM|DELAY|DELKEYS|DEPND|DEPTH|DERIV|DERVX|DESOLVE|DET|DETACH|DIAGMAP|DIFF|DIFFEQ|DISP|DISPXY|distance|DISTRIB|DIV|DIV2|DIV2MOD|DIVIS|DIVMOD|DIVPC|DOERR|DOLIST|DOMAIN|DOSUBS|DOT|DRAW|DRAW3DMATRIX|DRAX|droite|DROITE|DROP|DROP2|DROPN|DTAG|DUP|DUP2|DUPDUP|DUPN|EDIT|EDITB|EGCD|EGV|EGVL|element|ENDSUB|ENG|enregistrer|EPSX0|EQNLIB|EQW|eqxpnt|ER|ERASE|ERR0|ERRM|ERRN|est|EULER|EVAL|EX&LN|EXLR|EXP|EXP2HYP|EXP2POW|EXPAN|EXPAND|EXPANDMOD|EXPFIT|EXPLN|EXPM|EYEPT|FACT|FACTOR|FACTORMOD|FACTORS|FANNING|FAST3D|FCOEF|FDISTRIB|FFT|FILER|FINDALARM|FINISH|FIX|FLASHEVAL|FLOOR|FONT6|FONT7|FONT8|FOURIER|FP|FREE|FREEZE|FROOTS|FUNCTION|FXND|GAMMA|GAUSS|GBASIS|GCD|GCDMOD|GET|GETADR|GETI|GETNAME|GETNAMES|GETNEAR|gmol|GOR|GRAD|GRAMSCHMIDT|GRAPH|GREDUCE|GRIDMAP|GROB|GROBADD|GXOR|HADAMARD|HALFTAN|HALT|hauteur|HEAD|HELP|HERMITE|HESS|HEX|HILBERT|HIPERBOLIC|HISTOGRAM|HISTPLOT|HOME|HORNER|IABCUV|IBASIS|IBERNOULLI|IBP|ICHINREM|IDIV2|IDN|IEGCD|IFFT|ILAP|IM|IMAGE|INCR|INDEP|INFORM|INPUT|INT|INTEGER|inter|INTVX|INV|inversion|INVMOD|IP|IQUOT|IREMAINDER|ISOL|ISOM|JORDAN|KER|KERRM|KEY|KEYEVAL|KGET|KILL|LABEL|LAGRANGE|LAP|LAPL|LAST|LASTARG|lbmol|LC~C|LCM|LCXM|LDEC|legende|LEGENDRE|LGCD|LIBEVAL|LIBS|lieu|lim|LIMIT|LIN|LINE|LINFIT|LININ|LINSOLVE|LN|LNAME|LNCOLLECT|LNP1|LOCAL|LOG|LOGFIT|LQ|LR~R|LR|LSQ|LU|LVAR|MAD|MAIN|MAKESTR|MANT|MAP|MATHS|MATR|MAX|MAXR|MCALC|MEAN|mediane|mediatrice|MEM|MENU|MENUXY|MERGE|milieu|MIN|MINEHUNT|MINIT|MINR|MITM|MKISOM|MOD|MODSTO|MODULAR|MROOT|MSGBOX|MSLV|MSOLVR|MULTMOD|MUSER|NDIST|NDUPN|NEG|NEWOB|NEXTPRIME|NIP|nop|NOT|NOVAL|NSUB|NUM|NUMX|NUMY|OCT|OFF|OLDPRT|OPENIO|OR|ORDER|ordonnee|OVER|P2C|PA2B2|PARAMETRIC|paramplot|PARITY|PARSURFACE|PARTFRAC|PATH|PCAR|PCOEF|PCONTOUR|PCOV|PDIM|PEEK|PEEKARM|PERM|perpendiculaire|PEVAL|PGDIR|PICK|PICK3|PICTURE|PICT|PINIT|PIXOFF|PIXON|PKT|plot|PLOT|PLOTADD|PMAX|PMIN|PMINI|point|POKE|POKEARM|POLAR|POLYNOMIAL|POP|POS|POTENTIAL|POWEXPAND|POWMOD|PR1|PREDV|PREDX|PREDY|PREVAL|PREVPRIME|PRLCD|projection|PROMPT|PROMPTSTO|PROOT|PROPFRAC|PRST|PRSTC|PRVAR|PSDEV|Psi|PSI|PTAYL|PURGE|PUSH|PUT|PUTI|PVAR|PVARS|PVIEW|PWRFIT|qr|QR|QUAD|quadrilatere|QUOT|QUOTE|QXA|R~SB|RAD|RAND|RANK|RANM|RATIO|rayon|RCEQ|RCI|RCIJ|RCL|RCLALARM|RCLF|RCLKEYS|RCLMENU|RCLVX|RCWS|RDM|RDZ|RE|RECN|RECT|RECV|REF|REMAINDER|RENAME|REORDER|REPL|RES|RESTORE|RESULTANT|REVLIST|REWRITE|RISCH|RKF|RKFERR|RKFSTEP|RL|RLB|RND|RNRM|ROLL|ROLLD|ROMUPLOAD|ROOT|ROT|rotation|rpm|RR|RRB|rref|RREF|RREFMOD|RRK|RRKSTEP|RSBERR|RSD|RSWP|RULES|S~N|SAME|SB~B|SBRK|SCALE|SCALEH|SCALEW|SCATRPLOT|SCATTER|SCHUR|SCI|SCONJ|SCROLL|SDEV|segment|SEND|SEQ|SERIAL|SERIES|SERVER|SEVAL|SF|SHOW|SIDENS|SIGMA|SIGMAVX|SIGN|SIGNTAB|similitude|SIMP2|SIMPLIFY|SIN|SINCOS|SINH|SINV|SIZE|SL|SLB|SLOPEFIELD|SNEG|SNRM|SOLVE|SOLVEQN|SOLVER|SOLVEVX|sommets|SORT|SPHERE|SQ|SR|SRAD|SRB|SRECV|SREPL|SREV|STD|STEQ|STIME|STO|STOALARM|STOF|STOKEYS|STORE|STOVX|STREAM|STURM|STURMAB|STWS|SUB|SUBST|SUBTMOD|SVD|SVL|SWAP|SYLVESTER|symetrie|SYSEVAL|SYST2MAT|TABVAL|TABVAR|TAIL|TAN|TAN2CS2|TAN2SC|TAN2SC2|tangent|TANH|TAYLOR0|TAYLR|TCHEBYCHEFF|TCOLLECT|TDELTA|TESTS|TEVAL|TEXPAND|TEXT|TICKS|TIME|TINC|TLIN|TLINE|TMENU|TOT|TRACE|TRAN|TRANSIO|translation|triangle|TRIG|TRIGCOS|TRIGO|TRIGSIN|TRIGTAN|TRN|TRNC|TRUNC|TRUTH|TSIMP|TSTR|TVARS|TVM|TVMBEG|TVMEND|TVMROOT|TYPE|UBASE|UFACT|UNASSIGN|UNASSUME|UNBIND|UNPICK|UNROT|UPDIR|UTPC|UTPF|UTPN|UTPT|UVAL|VANDERMONDE|VAR|VARS|VER|VERSION|VISIT|VISITB|VPOTENTIAL|VTYPE|WAIT|WIREFRAME|WSLOG|XCOL|XGET|XMIT|XNUM|XOR|XPON|XPUT|XQ|XRECV|XRNG|XROOT|XSEND|XSERV|XVOL|XXRNG|YCOL|YRNG|YSLICE|YVOL|YYRNG|ZEROS|ZFACTOR|ZVOL|zxpnt)\\b"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator.userrpl",
					"match": "\\+|\\-|\\*|/|<|>|=|\\^|!|%|\\?|\\|"
				}
			]
		}
	},
	"scopeName": "source.userrpl"
}